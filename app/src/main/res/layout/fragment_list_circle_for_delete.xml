<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListCircleForDeleteFragment"
    android:orientation="vertical">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_layout"/>
    <RelativeLayout
        android:id="@+id/pathRelativeOnDelete"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <fragment
            android:id="@+id/google_map_for_delete"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="55dp" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/help_button_for_delete"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/ic_baseline_help_outline_24"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_marginTop="120dp"/>

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/close_help_button_for_delete"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="450dp"
            android:background="@drawable/ic_baseline_close_24"
            android:visibility="gone"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_marginTop="120dp"/>

    </RelativeLayout>

</FrameLayout>